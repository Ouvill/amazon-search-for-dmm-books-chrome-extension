{
  "manifest_version": 2,
  "name": "DMM BOOKSからAmazonの商品を検索する",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://book.dmm.com/detail/*",
        "https://book.dmm.com/detail/*"
      ],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "web_accessible_resources": [
    "content.styles.css",
    "icon-128.png",
    "icon-34.png"
  ]
}
